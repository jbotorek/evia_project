<header id="site_head">    <div class="header_cont">      <h1><a href="#">eVia</a></h1>      <nav class="head_nav">        <ul>            <% if signed_in? %>                <li><%= link_to "My Profile", current_user %></li>                <% if !current_user.has_info? %>                    <li><%= link_to "Create myInfo", new_info_path %></li>                <% else %>                    <li><%= link_to "edit MyInfo", edit_info_path(current_user.id) %></li>                <% end %>                <li><%= link_to "Sign out", signout_path, method: "delete" %></li>              <% else %>                <li><%= link_to "Sign in", signin_path %></li>                <li><%= link_to "Home", root_path %></li>              <% end %>        </ul>      </nav>    </div></header>