<div class="container">	  <li><%= link_to "Home", root_path %></li>	  <% if signed_in? %>		<li><%= link_to "My Profile", current_user %></li>		<% if !current_user.has_info? %>			<li><%= link_to "Create myInfo", new_info_path %></li>		<% else %>			<li><%= link_to "edit MyInfo", edit_info_path(current_user.id) %></li>		<% end %>		<li><%= link_to "Users", users_path %></li>		<li><%= link_to "Create a new Route", new_route_path %></li>		<li><%= link_to "Sign out", signout_path, method: "delete" %></li>	  <% else %>		<li><%= link_to "Sign in", signin_path %></li>	  <% end %></div>