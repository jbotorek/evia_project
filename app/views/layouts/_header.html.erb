<header id="site_head">    <div class="header_cont">      <h1><%= link_to "eVia", current_user %></h1>      <nav class="head_nav">		        <ul>            <% if signed_in? %>                <li>					<%= image_tag("FunctionIcons/myProfile.png", :size => "20x20", :alt => "my profile") %>					<%= link_to "My Profile", current_user %>									</li>                <% if !current_user.has_info? %>                    <li>						<%= image_tag("FunctionIcons/info.png", :size => "20x20", :alt => "info") %>						<%= link_to "Create myInfo", new_info_path %>					</li>                <% else %>                    <li>						<%= image_tag("FunctionIcons/info.png", :size => "20x20", :alt => "info") %>						<%= link_to "edit MyInfo", edit_info_path(current_user.id) %>					</li>                <% end %>                <li>					<%= image_tag("FunctionIcons/logout.png", :size => "15x30", :alt => "logout") %>					<%= link_to "Sign out", signout_path, method: "delete" %>				</li>		    <% else %>			    <li>					<%= image_tag("FunctionIcons/login.png", :size => "20x20", :alt => "login") %>					<%= link_to "Sign in", signin_path %></li>				<li>					<%= image_tag("FunctionIcons/myProfile.png", :size => "20x20", :alt => "home") %>					<%= link_to "Home", root_path %>				</li>		  <% end %>        </ul>      </nav>    </div></header>